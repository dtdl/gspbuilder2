	<monitor.use  monitor="../Config/PreTaskCheck"/>
    <monitor.use  monitor="../Config/MailMonitor"/>
    <monitor.use  monitor="../Config/CheckJblgJobs"/>

	<run_time  time_zone="${schedule.timeZone}" single_start="${schedule.begin}">
        <weekdays >
            <day  day="${schedule.days}">
                <period  single_start="${schedule.begin}"/>
            </day>
        </weekdays>
    </run_time>
    
</job>