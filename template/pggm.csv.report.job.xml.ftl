        <mail_on_error ><![CDATA[no]]></mail_on_error>
        <mail_on_warning ><![CDATA[no]]></mail_on_warning>
        <mail_on_success ><![CDATA[no]]></mail_on_success>
    </settings>
    <params >
        <include  live_file="../Config/EmailParameters.xml"/>
        <param  name="task_name" value="${schedule.title}"/>
        <param  name="SLA" value="20"/>
        <param  name="file_name" value=""/>
        <param  name="pre_task_name" value="${schedule.dependency}"/>
        <param  name="pre_task_count" value="${schedule.dependencyCount}"/>
		<param  name="forceMailFlag" value="false"/>
		<param  name="taskCheckFlag" value="true"/>
        <param  name="hibernate_file" value="$SCHEDULER_DATA/config/reporting.hibernate.cfg.xml"/>
    </params>

    <script  language="shell">
        <![CDATA[
$GOLD_KSH/TriggerEvent.ksh ${schedule.title} PGGMPublishDailyReport PUB
        ]]>
